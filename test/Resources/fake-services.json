{
  "services": {
    "foo": {
      "class": "./foo",
      "arguments": [
        "@bar",
        "%fs-extra",
        "foo-bar"
      ]
    },
    "bar": {
      "class": "./bar",
      "calls": [
        {
          "method": "setFooBar",
          "arguments": ["@foobar"]
        }
      ]
    },
    "foobar": {
      "class": "./foobar"
    }
  }
}